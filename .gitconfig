[alias]
        st = status
        co = checkout
        lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[user]
	name = <name>
	email = <email>
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = red
	untracked = cyan
